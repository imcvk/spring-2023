<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean class="com.beans.User" name="user">
        <property name="userId" value="23232"/>
        <property name="userName" value="sdfsd"/>
        <property name="vehicle" ref="vehicle"/>
    </bean>
    <bean class="com.beans.Vehicle" name="vehicle">
        <property name="vehicleId" value="234234"/>
        <property name="VName" value="sdfsdf"/>
    </bean>
</beans>