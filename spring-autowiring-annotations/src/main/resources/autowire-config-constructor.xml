<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context https://www.springframework.org/schema/context/spring-context.xsd">
    <context:annotation-config/>
    <bean class="constructor.beans.Student" name="student" id="student">
        <property name="name" value="fsdfsf"/>
        <property name="emailId" value="asdad@gmail.com"/>
    </bean>
    <bean class="constructor.beans.Address" id="address" name="address">
        <property name="city" value="Pune1"/>
        <property name="state" value="MH1"/>
        <property name="country" value="IND1"/>
    </bean>
    <bean class="constructor.beans.Address" id="address1" name="address1">
        <property name="city" value="Pune"/>
        <property name="state" value="MH"/>
        <property name="country" value="IND"/>
    </bean>
</beans>